<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="br.com.trabalho.ia">

    <application
        android:name=".config.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".ui.ResultPerfilActivity" />
        <activity android:name=".ui.extra.QuestionExtraCurricular" />
        <activity android:name=".ui.familia.QuestionFamilia" />
        <activity android:name=".ui.reforco.QuestionReforco" />
        <activity android:name=".ui.autodidata.QuestionAutodidata" />
        <activity android:name=".ui.Dashboard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>